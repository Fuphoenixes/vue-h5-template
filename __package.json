{
  "name": "{{name}}",
  "version": "{{version}}",
  "description": "{{description}}",
  "author": "{{author}}",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "npm run lint-js && npm run lint-style",
    "lint:fix": "npm run lint-js -- --fix && npm run lint-style -- --fix",
    "lint-js": "eslint --ext .js,.vue src",
    "lint-js:fix": "npm run lint-js -- --fix",
    "lint-style": "stylelint src/**/*.{html,vue,css,less}",
    "lint-style:fix": "npm run lint-style -- --fix",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "core-js": "^2.6.11",
    "qiniu-js": "^2.5.4",
    "v-loading-plugin": "^1.0.8",
    "vant": "^2.10.11",
    "vue": "^2.6.12",
    "vue-router": "^3.4.8",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.9.1",
    "@vue/cli-service": "^3.8.0",
    "babel-eslint": "10.0.1",
    "babel-plugin-import": "^1.13.1",
    "compression-webpack-plugin": "^3.1.0",
    "css-properties-sorting": "^1.0.10",
    "eslint": "5.15.3",
    "eslint-plugin-vue": "5.2.2",
    "less": "^3.12.2",
    "less-loader": "^4.1.0",
    "lint-staged": "8.1.5",
    "postcss-plugin-px2rem": "^0.8.1",
    "stylelint": "^12.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "svg-sprite-loader": "4.1.3",
    "svgo": "1.2.2",
    "vconsole": "3.3.4",
    "vconsole-webpack-plugin": "^1.5.2",
    "vue-template-compiler": "^2.6.12",
    "webpack-api-mocker": "^1.7.6",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{html,vue,css,less}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
